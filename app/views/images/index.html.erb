<%=link_to "New Image", new_image_path, :class => "button"%>
<br><br>
<div class="images">
<% @images.each do |image| %>
	<%= link_to "http://www.likeawritingdesk.com" + image.image.url, image_path(image) %>
	<%= image_tag image.image.url%>
	<%= link_to "Delete", image_path(image), :class => "negative button", :method => "delete"%>
	<br><br>
<% end %>
</div>